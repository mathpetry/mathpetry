programa {
  funcao inicio() {
    //algoritmo queima de estoque
  caracter nome
  inteiro codigo
  real valor
  inteiro quantidade
  real valortotal 
  escreva ("digite o nome da peça: ")
  leia (nome)
  escreva ("digite o codigo da peça: ")
  leia (codigo)
  escreva ("digite o valor unitario da peça: ")
  leia (valor)
  escreva ("digite a quantidade de peças: ")
  leia (quantidade)
  valortotal=quantidade*valor
  escreva ("valor total ", valortotal)
  
  }
}
